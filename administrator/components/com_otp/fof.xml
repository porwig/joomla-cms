<?xml version="1.0" encoding="UTF-8"?>
<fof>
	<backend>
		<dispatcher>
			<option name="default_view">users</option>
		</dispatcher>
		<view name="user">
			<acl>
				<task name="*">false</task>
				<task name="browse">core.admin</task>
				<task name="edit">core.admin</task>
				<task name="save">core.admin</task>
				<task name="apply">core.admin</task>
			</acl>
		</view>
	</backend>
	
	<frontend>
		<dispatcher>
			<option name="default_view">user</option>
		</dispatcher>
		<view name="*">
			<acl>
				<task name="*">false</task>
			</acl>
		</view>
		<view name="user">
			<acl>
				<task name="apply"></task>
				<task name="read"></task>
				<task name="save"></task>
			</acl>
		</view>
	</frontend>
</fof>
